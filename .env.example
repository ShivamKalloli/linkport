# LinkPort API Configuration
# Copy this file to .env and fill in your actual API keys

# Spotify API (Required for Spotify playlist parsing)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# YouTube Data API v3 (Required for YouTube Music playlist parsing)
YOUTUBE_API_KEY=your_youtube_api_key_here

# SoundCloud API (Required for SoundCloud playlist parsing)
SOUNDCLOUD_CLIENT_ID=your_soundcloud_client_id_here

# Optional: Rate limiting and caching
REDIS_URL=your_redis_url_here

# Environment
NODE_ENV=development
PORT=3001